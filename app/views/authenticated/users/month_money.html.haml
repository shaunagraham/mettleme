:javascript
  $(document).ready(function(){

      // Selct Box
      $(document).click(function(e){if(!$(e.target).is('.selection')){$(".sel_me").slideUp(200)}});

      $('.select .selection').click(function(){
          $('.sel_me').slideUp();
          if ($(this).parent('').children('.sel_me').is(':visible')){
              $(this).parent('').children('.sel_me').slideUp(200);
          }else{
              $(this).parent('').children('.sel_me').slideToggle(200);
          }
      });

      $('ul.sel_month > li').click(function(){
          $('input.sel_month').val($(this).html());
          $('ul.sel_month').slideToggle(200);
      });

      $('.parent_table .content_row .toggle_icon').click(function(){
          if ($(this).parent('td').parent('.content_row').parent('tbody').parent('.parent_table').children('tbody').children('tr').children('td').children('.child_table').is(':visible')){
              $(this).parent('td').parent('.content_row').parent('tbody').parent('.parent_table').children('tbody').children('tr').children('td').children('.child_table').slideUp(0);
              $(this).parent('td').parent('.content_row').parent('tbody').parent('.parent_table').removeClass('active');
          }else{
              $(this).parent('td').parent('.content_row').parent('tbody').parent('.parent_table').children('tbody').children('tr').children('td').children('.child_table').slideToggle(0);
              $(this).parent('td').parent('.content_row').parent('tbody').parent('.parent_table').addClass('active');
          }

      });
  });